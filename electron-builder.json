{
    "appId": "com.mugambabruno.lwzikamp",
    "productName": "Lw-Ziki-Amp",
    "copyright": "COPYRIGHT MUGAMBA BRUNO",
    "directories": {
        "app": "./",
        "output": "build",
        "buildResources": "./src/assets"
    },
    "files": [
        "./package.json",
        "./node_modules",
         "./dist",
         "frame.js"
         
         
    ],
   
    "dmg": {
        "backgroundColor": "#080808",
        "window": {
            "width": "750",
            "height": "500"
        },
        "contents": [
            {
                "x": 100,
                "y": 100
            },
            {
                "x": 300,
                "y": 100,
                "type": "link",
                "path": "/Applications"
            }
        ]
    },
    "mac": {

        "target": "mas",
        "category": "public.app-category.utilities"
      },
    "win": {
        "target": "nsis",
         "icon": "./src/assets/pAudio.png"
    },
    "linux": {
        "target":["AppImage","snap"],
        "icon":"./src/assets/pAudio.png"
    }
}
